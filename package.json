{
  "name": "oort-cloud",
  "productName": "Oort Cloud",
  "version": "1.0.0",
  "description": "Browser for secured browsing",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "author": "Adithiyan R",
  "license": "MIT",
  "devDependencies": {
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.exam.securebrowser",
    "productName": "Oort Cloud",
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "icon": "build/icons/OortCloud.icns"
    },
    "win": {
      "icon": "build/icons/OortCloud.ico"
    },
    "linux": {
      "icon": "build/icons/OortCloud.png"
    }
  }
}
